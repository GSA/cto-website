{
  "name": "cto-website",
  "description": "Website for the GSA Office of the Chief Technology Officer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GSA/cto-website.git"
  },
  "author": "GSA",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/GSA/cto-website/issues"
  },
  "homepage": "https://github.com/GSA/cto-website#readme",
  "scripts": {
    "start": "concurrently npm:cms-proxy npm:serve",
    "serve": "bundle exec jekyll serve",
    "cms-proxy": "netlify-cms-proxy-server",
    "test": "npm run test:eslint && npm run test:links:internal && npm run test:a11y:local",
    "test:serve": "bundle exec jekyll serve -P 54321",
    "test:eslint": "eslint 'assets/js/**/*.js'",
    "test:links:internal": "start-server-and-test test:serve 54321 'blc -e -r --exclude docx --exclude xlsx --exclude pptx --exclude pdf --exclude svg http://localhost:54321'",
    "test:links:external": "start-server-and-test test:serve 54321 'blc -i -r http://localhost:54321'",
    "test:a11y:local": "start-server-and-test test:serve 54321 'pa11y-ci --sitemap http://localhost:54321/sitemap.xml'",
    "test:a11y:live": "pa11y-ci --sitemap https://tech.gsa.gov/sitemap.xml"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.3",
    "@babel/core": "^7.17.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@uswds/compile": "^1.0.0-alpha.0",
    "autoprefixer": "^10.4.2",
    "broken-link-checker": "^0.7.8",
    "concurrently": "^7.0.0",
    "eslint": "^8.9.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svg-sprite": "^1.5.0",
    "netlify-cms-proxy-server": "^1.3.23",
    "pa11y-ci": "^3.0.1",
    "postcss": "^8.4.6",
    "postcss-csso": "^6.0.0",
    "sass": "^1.49.7",
    "start-server-and-test": "^1.14.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "extend": "^3.0.2",
    "jquery": "^3.6.0",
    "lunr": "^2.3.9",
    "mousetrap": "^1.6.5",
    "netlify-cms": "^2.10.186",
    "react": "^17.0.2",
    "uswds": "^2.13.1",
    "waypoints": "^4.0.1"
  }
}
